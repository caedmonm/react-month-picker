{
  "name": "simple-react-month-picker",
  "version": "1.1.15",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "type": "module",
  "sideEffects": [
    "**/*.css"
  ],
  "files": [
    "dist",
    "package.json",
    "README.md"
  ],
  "scripts": {
    "build": "npm run build:css && tsc",
    "build:css": "tailwindcss -i ./src/styles/tailwind.css -o ./dist/styles.css --minify",
    "build:css:watch": "tailwindcss -i ./src/styles/tailwind.css -o ./dist/styles.css --watch",
    "build:watch": "npm run build:css && tsc --watch",
    "clean": "rimraf dist",
    "prepublishOnly": "npm run clean && npm run build"
  },
  "keywords": [
    "react",
    "calendar",
    "monthpicker",
    "month picker",
    "date picker",
    "months"
  ],
  "author": "CAEDMON MULLIN <caedmon.m@gmail.com> (https://caedmonmullin.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/caedmonm/react-month-picker/issues"
  },
  "homepage": "https://github.com/caedmonm/react-month-picker",
  "peerDependencies": {
    "react": ">=16.8.0",
    "react-dom": ">=16.8.0"
  },
  "devDependencies": {
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "autoprefixer": "^10.4.21",
    "postcss": "^8.5.6",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "rimraf": "^6.0.1",
    "tailwindcss": "^3.4.18",
    "typescript": "^5.6.3"
  }
}
